{
    "boot-source": {
        "kernel_image_path": "images/vmlinux-6.12",
        "boot_args": "console=ttyS0 reboot=k panic=1 pci=off ip=192.168.1.201::192.168.1.1:255.255.255.0::eth0:off init=/swim/init"
    },
    "drives": [
        {
            "drive_id": "vda",
            "path_on_host": "/tmp/rootfs",
            "is_root_device": true,
            "is_read_only": false
        },
        {
            "drive_id": "vdb",
            "path_on_host": "images/ubuntu.img",
            "is_root_device": false,
            "is_read_only": false
        }
    ],
    "network-interfaces": [
        {
            "iface_id": "eth0",
            "host_dev_name": "firetap0"
        }
    ],
    "machine-config": {
        "vcpu_count": 1,
        "mem_size_mib": 256
    }
}
